<h1>Users</h1>   

<p>
  <a href="/admins/new">Create new admin</a>
</p>

<ul>
  <% for(var username in users) { %>
    <li><% users[username].username %></li>
  <% } %>

  <% if (page > 0) { %>
    <a href="?page=<%(page - 1)%>">Previous</a>
    &nbsp;
  <% } %>

  <% if (!lastPage) { %>
    <a href="?page=<%(page + 1)%>">Next</a>
  <% } %>
</ul>